<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	jaxb:extensionBindingPrefixes="xjc" version="2.1">

	<jaxb:globalBindings>
		<jaxb:javaType xmlType="xs:dateTime" name="java.util.Date"
			parseMethod="io.github.learnjaxws.ws.adapters.DataTypeAdapter.parseDateTime"
			printMethod="io.github.learnjaxws.ws.adapters.DataTypeAdapter.printDateTime" />
		<jaxb:javaType xmlType="xs:date" name="java.util.Date"
			parseMethod="io.github.learnjaxws.ws.adapters.DataTypeAdapter.parseDate"
			printMethod="io.github.learnjaxws.ws.adapters.DataTypeAdapter.printDate" />
	</jaxb:globalBindings>

</jaxb:bindings>